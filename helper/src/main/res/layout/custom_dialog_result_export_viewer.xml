<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/ll_custom_dialog_result_export_viewer_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_custom_dialog_result_export_viewer_exit"
        android:layout_width="@dimen/CustomDialogActionResultSizeExit"
        android:layout_height="@dimen/CustomDialogActionResultSizeExit"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/command_end_bottom_margin"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/pic_export" />

</RelativeLayout>